<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/linearRowrectangletwelve"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:background="@android:color/white"
    android:padding="10dp">

    <ImageView
        android:id="@+id/imageRectangleTwelve"
        android:layout_width="96dp"
        android:layout_height="104dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        android:background=""
        android:scaleType="fitXY"
        android:src="@drawable/banner1" />

    <LinearLayout
        android:id="@+id/linearColumnsignal"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="0.7"
        android:layout_marginTop="10dp"
        android:layout_marginStart="10dp"
        android:layout_marginBottom="13dp"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/linearRowsignal1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/linearRowsignal2"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.35"
                android:orientation="horizontal"
                android:layout_marginTop="1dp"
                android:layout_marginBottom="1dp">

                <ImageView
                    android:id="@+id/imageSignal"
                    android:layout_width="13dp"
                    android:layout_height="13dp"
                    android:layout_marginTop="2dp"
                    android:layout_marginBottom="2dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/rating"
                    />

                <TextView
                    android:id="@+id/txtFortyNine"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.56"
                    android:singleLine="true"
                    android:ellipsize="end"
                    android:text="4.9"
                    style="@android:style/TextAppearance.Small"
                    />

            </LinearLayout>

            <TextView
                android:id="@+id/txtFrameSixtySeven"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="104dp"
                android:layout_weight="0.65"
                android:singleLine="true"
                android:ellipsize="end"
                android:paddingStart="6dp"
                android:paddingEnd="6dp"
                android:paddingTop="4dp"
                android:paddingBottom="1dp"
                android:text="Apartment"
                style="@style/TextAppearance.AppCompat.Small"
                />
        </LinearLayout>

        <TextView
            android:id="@+id/txtWoodlandApartm"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="10dp"
            android:singleLine="true"
            android:ellipsize="end"
            android:text="Woodland Apartment"
            style="@android:style/TextAppearance.Medium" />

        <LinearLayout
            android:id="@+id/linearRowlinkedin"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="2dp"
            android:layout_marginEnd="10dp">

            <ImageView
                android:id="@+id/imageLinkedinTwo"
                android:layout_width="8dp"
                android:layout_height="10dp"
                android:scaleType="fitXY"
                android:src="@drawable/finder"
                />

            <TextView
                android:id="@+id/txt1012Oceanavan"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.96"
                android:singleLine="true"
                android:ellipsize="end"
                android:text="1012 Ocean Avan"
                style="@android:style/TextAppearance.Small" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearRowtelevision"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="7dp"
            android:layout_marginEnd="10dp">

            <ImageView
                android:id="@+id/imageTelevision"
                android:layout_width="12dp"
                android:layout_height="9dp"
                android:layout_marginTop="4dp"
                android:layout_marginBottom="2dp"
                android:scaleType="fitXY"
                android:src="@android:drawable/ic_menu_camera"
                />

            <TextView
                android:id="@+id/txt1225"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.18"
                android:singleLine="true"
                android:ellipsize="end"
                android:text="12:25"
                style="@android:style/TextAppearance.Small"
                />

            <ImageView
                android:id="@+id/imageCar"
                android:layout_width="13dp"
                android:layout_height="9dp"
                android:layout_marginTop="4dp"
                android:layout_marginStart="12dp"
                android:layout_marginBottom="2dp"
                android:scaleType="fitXY"
                android:src="@android:drawable/ic_menu_gallery"
                />

            <TextView
                android:id="@+id/txtThirty"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.11"
                android:singleLine="true"
                android:ellipsize="end"
                android:text="30"
                style="@android:style/TextAppearance.Small" />

            <TextView
                android:id="@+id/txtPriceTwo"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.52"
                android:singleLine="true"
                android:ellipsize="end"
                android:text="ksh6000/month"
                style="@android:style/TextAppearance.Medium" />
        </LinearLayout>
    </LinearLayout>
</LinearLayout>
